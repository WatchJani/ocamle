let swap_upper_lower znak=match znak with
  | 'a'..'z'->char_of_int ((int_of_char znak)-32)
  | 'A'..'Z'->char_of_int((int_of_char znak)+32)
  | _->znak;;

swap_upper_lower 'g';;


let rec iterate f x =function
  | n when n<=0 -> x
  | n ->iterate f (f x) (n-1);;

iterate (fun y->1::y) [] 5;;

let potenca osnova exponent=iterate (fun y-> y * osnova) 1 exponent;;
potenca 2 10;;

