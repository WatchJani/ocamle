Array.make 6 "";;
let od_n_do_m n m=
  let polje=Array.make (abs(m-n)+1) n in
  if n <=m then
    (for i = n+1 to m do
       polje.(i-n)<-i
     done)
  else
    (for i = n -1 downto m do
       polje.(n-1)<-i
     done)
      polje;;
od_n_do_m 4 7;;
