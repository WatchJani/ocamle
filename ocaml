  print_int i; print_string ";"
done; print_string "\n";;


for i=1 downto 5 do 
  print_int i; print_string ";"
done; print_string "\n";;

let pescena_ura n = 
  for i=1 to n do
    for j = 1 to n do
      if ( i = 1 || i=n || j>=i || j+i<=n+1 ) then print_string "*" else print_string " "; 
    done; print_string "\n";
  done;;

pescena_ura 30;;

